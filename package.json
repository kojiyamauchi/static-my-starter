    "webpack-stream": "^6.1.2"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "modules": false,
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "lint-staged": {
    "resource/**/*.{ts,js}": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ],
    "resource/**/*.scss": "stylelint --fix",
    "webpack/*.js": "prettier --write",
    "gulpfile.babel.js": "prettier --write"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleNameMapper": {
      "^@/(.+)": "<rootDir>/resource/base/$1"
    },
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/resource/tests/**/*.test.+(ts|js)"
    ]
  },
  "scripts": {
    "setup": "rm -rf .git && touch delivery/index.html",
    "prepare": "husky install",
    "dev": "gulp",
    "build": "gulp onBuild",
    "serve": "gulp onBrowserSync",
    "clean": "gulp onClean && touch delivery/index.html",
    "jest": "jest --passWithNoTests",
    "check-pkg": "ncu",
    "update-pkg": "ncu '/^(?!(add-module-name|@add-module-name)).*$/' -u",
    "re-install": "rm yarn.lock && rm -rf node_modules && yarn install"
  }
}
